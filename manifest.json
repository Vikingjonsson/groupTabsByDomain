{
  "manifest_version": 3,
  "name": "Group Tabs by Domain",
  "version": "1.0.0",
  "description": "Automatically groups tabs by domain to keep your browsing organized",
  "permissions": ["tabs", "tabGroups"],
  "background": {
    "service_worker": "background.js"
  },
    "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
